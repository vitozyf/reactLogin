{ 
  "name" : "reactlogin",
  "script" : "./server-wrapper.js",
  "cwd" : "./",
  "watch": [
    "src"
  ], 
  "ignore_watch" : [
    "node_modules", "logs", "src/test" 
  ], 
  "watch_options": { 
    "followSymlinks": false 
  }, 
  "error_file" : "./logs/app-err.log",
  "out_file" : "./logs/app-out.log",
  "env": { 
    "NODE_ENV": "production"
  } 
}